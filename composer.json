        "phpunit/phpunit": "^4.0|^5.0|^6.0",
        "josegonzalez/dotenv": "3.1.0",
        "satooshi/php-coveralls": "~1.0",
        "mockery/mockery": "0.9.*"
    },
    "autoload": {
        "psr-4": {
